<?xml version="1.0" encoding="UTF-8"?>
<project name="MARC4J" xmlns="http://maven.apache.org/DECORATION/1.3.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/DECORATION/1.3.0 http://maven.apache.org/xsd/decoration-1.3.0.xsd">

  <bannerLeft>
    <name>MARC4J</name>
    <href>http://ksclarke.github.io/marc4j/</href>
  </bannerLeft>

  <publishDate position="bottom" format="yyyy-MM-dd"/>
  <version position="bottom"/>
  <googleAnalyticsAccountId/>

  <skin>
    <groupId>lt.velykis.maven.skins</groupId>
    <artifactId>reflow-maven-skin</artifactId>
    <version>1.0.0</version>
  </skin>
  <custom>
    <reflowSkin>
      <theme>bootswatch-spacelab</theme>
      <protocolRelativeURLs>false</protocolRelativeURLs>
      <smoothScroll>true</smoothScroll>
      <highlightJs>false</highlightJs>
      <brand>
        <name>MARC4J </name>
        <href>http://ksclarke.github.io/marc4j/</href>
      </brand>
      <slogan>An easy to use API for working with MARC and MARCXML in
        Java</slogan>
      <titleTemplate>%2$s | %1$s</titleTemplate>
      <toc>false</toc>
      <tocTopMax>6</tocTopMax>
      <!-- Include the documentation and tools in the top navigation (in addition to links) -->
      <topNav>Documentation</topNav>
      <bottomNav maxSpan="12">
        <column>Build Status|Project Links|Site Copyright</column>
        <column>Developer Reports</column>
        <column>reports|Maven Theme</column>
      </bottomNav>
      <pages>
        <index project="marc4j">
          <titleTemplate>MARC4J</titleTemplate>
          <breadcrumbs>false</breadcrumbs>
          <toc>false</toc>
          <markPageHeader>true</markPageHeader>
          <sections>
            <columns>2</columns>
            <body/>
          </sections>
        </index>
      </pages>
      <skinAttribution>false</skinAttribution>
    </reflowSkin>
  </custom>

  <body>
    <head> </head>

    <links>
      <item name="Download ↗" href="http://github.com/ksclarke/marc4j/releases"
        target="_new"/>
      <item name="GitHub project ↗" href="http://github.com/ksclarke/marc4j"
        target="_new"/>
    </links>

    <!-- top navbar -->
    <menu name="Documentation">
      <item name="Tutorial" href="tutorial.html"/>
      <item name="Javadocs ↗" href="apidocs/index.html" target="_new"/>
      <item name="Issues Queue ↗" target="_new"
        href="http://github.com/ksclarke/marc4j/issues"/>
      <item name="License" href="license.html"/>
    </menu>

    <!-- bottom navbar -->
    <menu name="Project Links">
      <item name="Home Page" href="index.html#"/>
      <item name="Contact Info" href="team-list.html"/>
      <item name="Brief Summary" href="project-summary.html"/>
    </menu>
    <menu name="Developer Reports">
      <item name="FindBugs Report" href="findbugs.html"/>
      <item name="Test Coverage ↗" href="cobertura/index.html" target="_new"/>
      <item name="PMD Report" href="pmd.html"/>
      <item name="CPD Report" href="cpd.html"/>
      <item name="JDepend Metrics" href="jdepend-report.html"/>
      <item name="Change Log" href="changelog.html"/>
      <item name="Tag List" href="taglist.html"/>
    </menu>
    <menu name="Build Status">
      <item img="https://travis-ci.org/marc4j/marc4j.png" name=" ↗"
        href="https://travis-ci.org/marc4j/marc4j"/>
    </menu>
    <menu name="Maven Docs" ref="reports"/>
    <menu name="Maven Theme">
      <item name="Maven Reflow Skin" target="_new"
        href="http://andriusvelykis.github.io/reflow-maven-skin/"/>
    </menu>
    <menu name="Site Copyright">
      <item name="GNU Free Doc License ↗" target="_new"
        href="http://www.gnu.org/licenses/fdl-1.3-standalone.html"/>
    </menu>
  </body>
</project>
